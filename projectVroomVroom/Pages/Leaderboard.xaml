<UserControl x:Class="projectVroomVroom.Pages.Leaderboard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:projectVroomVroom.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="1080" d:DesignWidth="1920" Background="#646464">

    <UserControl.Resources>
        <Style x:Key="ArrowButtonStyle" TargetType="Button">
            <Setter Property="Background">
                <Setter.Value>
                    <ImageBrush ImageSource="/images/arrowbutton.png"/>
                </Setter.Value>
            </Setter>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Grid Background="{TemplateBinding Background}">
                            <ContentPresenter />
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </UserControl.Resources>

    <Viewbox StretchDirection="Both" Stretch="Fill">
        <Grid Width="1920" Height="1080">

            <Image Name="Map" Width="383" Height="145" Margin="365,314,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Source="/Images/map1.png" Stretch="Fill" RenderTransformOrigin="-0.65,1.291" />

            <Button Width="249" Height="75" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="25,0,0,25" Background="#B1B1B1" BorderBrush="Black" Content="TERUG" FontSize="48" FontFamily="Raleway" FontWeight="Bold" BorderThickness="2,2,2,2" Click="BackButtonClick"/>


            <Button Width="143" Height="89" Margin="475,526,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Style="{StaticResource ArrowButtonStyle}" RenderTransformOrigin="0.5,0.5" BorderBrush="{x:Null}" Click="LeaderboardMapDownArrowButtonClick" >
                <Button.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="89.685"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Button.RenderTransform>
                <Button.Background>
                    <ImageBrush ImageSource="/images/arrowbutton.png"/>
                </Button.Background>
            </Button>
            <Button Width="143" Height="89" Margin="477,186,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Style="{StaticResource ArrowButtonStyle}" BorderBrush="{x:Null}" Click="LeaderboardMapUpArrowButtonClick" RenderTransformOrigin="0.5,0.5" >
                <Button.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="-90.54"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Button.RenderTransform>
                <Button.Background>
                    <ImageBrush ImageSource="/Images/arrowbutton.png"/>
                </Button.Background>
